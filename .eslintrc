{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "complexity": ["error", 5],
    "no-bitwise": "warn",
    "no-console": "off",
    "no-plusplus": "off",
    "no-underscore-dangle": "off",
    "prefer-arrow-callback": "off",
    "consistent-return": "off",
    "comma-dangle": "off",
    "object-curly-spacing": "off",
    "strict": "off",
    "require-yield": "off",
    "no-whitespace-before-property": "off",
    "no-param-reassign": ["error", { "props": false }],
    "no-unexpected-multiline": "off",
    "no-throw-literal": "off",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],

    "prefer-rest-params": "off",
    "prefer-spread": "off"
  },
  "globals": {
    "co": true,
    "coLaunch": true,
    "coNext": true,
    "coNextGenerator": true,
    "coNextPromise": true,
    "ctorFunction": true,
    "ctorGenerator": true,
    "ctorIterator": true,
    "ctorObject": true,
    "current": true,
    "delay": true,
    "halt": true,
    "isPromise": true,
    "samficVersion": true,
    "status": true
  }
}
